{{> head }}

<nav>
    <div class="nav-wrapper grey lighten-4 purple-text text-darken-4">
        <img class='brand-logo abc-icon' src='img/abc_icon.png' alt='Logo'>
        <span style='margin-left: 80px'><strong>AFRICA BUSINESS</strong> Consulting</span>
    </div>
</nav>

<div class="row">
    <div class="col s12">
        <div class="card card-pre">
            <form method='post' action='#'>
                <div class="card-content">
                    <div class="input-field">
                        <input id="name" type="text" required>
                        <label for="name">Nome</label>
                    </div>
                    <div class="input-field">
                        <input id="address" type="text">
                        <label for="addres">Morada</label>
                    </div>
                    <div class="input-field">
                        <input id="phone" type="text" required>
                        <label for="phone">Contacto</label>
                    </div>
                    <div class="input-field">
                        <input id="email" type="email" required>
                        <label for="email">Email</label>
                    </div>
                </div>
                <div class="card-action">
                    <button type='submit' class="btn waves-effect waves-light grey">Pre Inscrever</button>
                </div>
            </form>
        </div>
    </div>
</div>